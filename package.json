{
  "name": "veddbe",
  "version": "0.0.0",
  "description": "A medical aid app",
  "main": "index.js",
  "scripts": {
    "build:css": "./node_modules/.bin/cleancss -o public/css/app.css src/css/app.css",
    "watch:css": "watch 'npm run build:css' src/css",
    "build:js": "./node_modules/.bin/babel src/js -d public/js --ignore src/jsx/**.jsx",
    "build:jsx": "./node_modules/.bin/browserify src/jsx/**.jsx -o public/js/bundle.js -t [ babelify --presets [ es2015 react  ] ]",
    "watch:js": "watch 'npm run build:js' src/js",
    "watch:jsx": "watch 'npm run build:jsx' src/jsx",
    "build:html": "./node_modules/.bin/jade -H -P src/template -o public",
    "watch:html": "watch 'npm run build:html' src/template",
    "build": "npm run build:js && npm run build:css && npm run build:html && npm run build:jsx",
    "watch": "parallelshell 'npm run watch:js' 'npm run watch:jsx' 'npm run watch:html' 'npm run watch:css'",
    "test": "node_modules/karma/bin/karma start karma.conf.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/kinja-js-class/veddbe.git"
  },
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/kinja-js-class/veddbe/issues"
  },
  "devDependencies": {
    "babel": "^6.3.13",
    "babel-cli": "^6.3.15",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.5.0",
    "babel-runtime": "^6.3.13",
    "clean-css": "^3.4.8",
    "jade": "^1.11.0",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.15",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^0.2.1",
    "parallelshell": "~2.0.0",
    "phantomjs": "^1.9.19",
    "watch": "^0.16.0"
  },
  "dependencies": {
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7"
  }
}
